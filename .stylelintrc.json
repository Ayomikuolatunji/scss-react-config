{
    "rules": {
        "block-no-empty": null,
        "unit-allowed-list": ["em", "rem", "%", "s","px","cm","mm","in","pt","pc","ch","vw","vh","vmin","vmax"],
        "unit-no-unknown": true
      },
    "plugins":["stylelint-prettier"],
    "extends":[
        "stylelint-config-sass-guidelines",
        "stylelint-config-prettier",
        "stylelint-prettier/recommended"
    ]
}